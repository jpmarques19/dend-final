						"     skipDuplicateMapOutputs: true) ~> sqlpayrolldata2021mapped",
						"sqlpayrolldata2021mapped, sqlpayrolldata2020 union(byName: true)~> union1",
						"union1 filter(toInteger(FiscalYear) >= $dataflow_param_fiscalyear) ~> filter1",
						"filter1 derive(TotalPaid = RegularGrossPaid + TotalOTPaid+TotalOtherPay) ~> deriveTotalPaid",
						"deriveTotalPaid aggregate(groupBy(AgencyName,",
						"          FiscalYear),",
						"     TotalPaid = sum(TotalPaid)) ~> aggregate1",
						"aggregate1 select(mapColumn(",
						"          FiscalYear,",
						"          AgencyName,",
						"          TotalPaid",
